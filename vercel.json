{
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "framework": "bun",
  "functions": {
    "src/index.tsx": {
      "runtime": "bun"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/index.tsx"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
